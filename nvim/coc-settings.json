{
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "python",
        "json",
        "markdown"
    ],
    "list.nextKeymap": "<c-n>",
    "list.previousKeymap": "<c-p>",
    "codeLens.enable": true,
    // "suggest.floatConfig": {
    //     "border": true
    // },
    // "diagnostic.floatConfig": {
    //     "border": true
    // },
    // "signature.floatConfig": {
    //     "border": true
    // },
    "hover.floatConfig": {
        "border": true
    },
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    // "diagnostic.virtualText": true,
    // "diagnostic.virtualTextPrefix": "   ■ ",
    // "diagnostic.virtualTextCurrentLineOnly": true,
    "diagnostic.enableHighlightLineNumber": false,
    "diagnostic.warningSign": "", // "•"
    "diagnostic.errorSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.hintSign": "",
    "snippets.ultisnips.directories": [
        "friendly-snippets"
    ],
    "git.changedSign.text": "▎",
    "git.addedSign.text": "▎",
    "git.removedSign.text": "▎",
    "git.changeRemovedSign.text": "▎",
    "git.addedSign.hlGroup": "GitAdd",
    "git.removedSign.hlGroup": "GitDelete",
    "git.changedSign.hlGroup": "GitChange",
    "git.topRemovedSign.hlGroup": "GitDelete",
    "git.changeRemovedSign.hlGroup": "GitDelete",
    "tabnine.disable_filetypes": [
        "python"
    ],
    "Lua.diagnostics.globals": [
        "vim"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--ignore=E501,E203"
    ],
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "-l 120"
    ],
    "texlab.build.args": [
        "-interaction=nonstopmode",
        "-synctex=1",
        "-file-line-error",
        "-lualatex",
        "-outdir=out",
        "%f"
    ],
    // "highlight.colorNames.enable": false,
}
